{
  "name": "nikahan_backend",
  "version": "1.0.0",
  "description": "Niika.id backend application",
  "main": "server.js",
  "author": "izzasrv",
  "license": "ISC",
  "scripts": {
    "start": "node dist/server.js",
    "build": "rm -rf dist && babel src --out-dir dist --copy-files",
    "postinstall": "npm run build",
    "dev": "nodemon --exec babel-node src/server.js",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint . --ext .js,.ts,.tsx,.json",
    "lint:fix": "eslint . --ext .js,.ts,.tsx,.json --fix",
    "prettier": "prettier --check '*/**/*.{js,ts,tsx,json}'",
    "prettier:fix": "prettier --write '*/**/*.{js,ts,tsx,json}'",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "app-root-path": "^3.0.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.3",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "envalid": "^7.2.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "joi-phone-number": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.6.5",
    "randomstring": "^1.2.1",
    "request-ip": "^2.1.3",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sharp": "^0.29.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "write": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@types/jest": "^26.0.24",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2"
  },
  "engines": {
    "node": "14.x"
  }
}
